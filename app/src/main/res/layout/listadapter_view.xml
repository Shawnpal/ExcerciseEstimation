                android:id="@+id/txtEResult18"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="LEar:" />
        </TableRow>


    </TableLayout>

    <TextView
        android:id="@+id/Resulttxt"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Result:" />

</LinearLayout>